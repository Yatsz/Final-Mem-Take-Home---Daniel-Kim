{
  "manifest_version": 3,
  "name": "Mem Extension",
  "author": "Daniel Kim",
  "description": "Saves the title and URL to MEM",
  "version": "2.0",
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Mem Extension",
    "default_icon": "icon.png"
  }
}